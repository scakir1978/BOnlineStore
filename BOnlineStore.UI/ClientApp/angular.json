{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "35c2a030-70e4-44b0-bcac-3c33ef95e5a5"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "velzon": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./webpack.config.js"
            },
            "outputPath": "dist/velzon",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "node_modules/shepherd.js/dist/css/shepherd.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "src/styles.scss",
              "node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "node_modules/swiper/swiper-bundle.css",
              "src/assets/scss/config/galaxy/bootstrap.scss",
              "src/assets/scss/config/galaxy/app.scss",
              "src/assets/scss/icons.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "ev-emitter/ev-emitter",
              "can-use-dom",
              "core-js/modules/es.array.filter",
              "core-js/modules/es.array.for-each",
              "ore-js/modules/es.array.index-of.js",
              "core-js/modules/es.object.assign",
              "core-js/modules/es.array.iterator",
              "core-js/modules/es.array.reduce",
              "core-js/modules/es.function.name",
              "core-js/modules/es.object.assig",
              "core-js/modules/es.object.to-string",
              "core-js/modules/es.parse-int",
              "core-js/modules/es.regexp.exec",
              "core-js/modules/es.regexp.to-string.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.string.iterator",
              "core-js/modules/es.string.match",
              "core-js/modules/es.string.replace",
              "core-js/modules/es.weak-map",
              "core-js/modules/web.dom-collections.iterator",
              "core-js/modules/es.string.ends-with.js",
              "core-js/modules/es.string.includes.js",
              "core-js/modules/es.string.starts-with.js",
              "core-js/modules/es.string.trim.js",
              "core-js/modules/es.function.name.js",
              "core-js/modules/es.function.name.js",
              "core-js/modules/es.object.assign.js",
              "core-js/modules/es.object.to-string.js",
              "core-js/modules/es.parse-int.js",
              "core-js/modules/es.regexp.exec.js",
              "core-js/modules/es.string.iterator.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.weak-map.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "raf",
              "rgbcolor",
              "browserify-cipher",
              "browserify-sign",
              "create-ecdh",
              "create-hash",
              "create-hmac",
              "diffie-hellman",
              "public-encrypt",
              "randombytes",
              "randomfill",
              "jszip",
              "devexpress-gantt",
              "devextreme-quill",
              "crypto",
              "crypto-js/core.js",
              "crypto-js/enc-base64.js",
              "crypto-js/enc-utf8.js",
              "crypto-js/sha256.js",
              "exceljs",
              "sweetalert2",
              "lodash.debounce",
              "lodash.memoize",
              "lodash.throttle",
              "rxjs/internal/operators",
              "moment",
              "lottie-web",
              "ngx-lightbox",
              "ngx-masonry",
              "file-saver",
              "oidc-client",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.string.split.js",
              "devexpress-diagram",
              "core-js/modules/es.array.filter.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.string.split.js",
              "devexpress-diagram",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.reduce.js'",
              "core-js/modules/es.array.reduce.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "velzon:build:development"
          },
          "configurations": {
            "production": {
              "browserTarget": "velzon:build:production"
            },
            "development": {
              "browserTarget": "velzon:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "velzon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  }
}
